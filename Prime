#include<stdio.h>
int main()
{
    int num,count,i;
    count=0;
    printf("Enter the number:");
    scanf("%d",&num);
    for(i=2;i<num;i++)
    {
        if(num%i==0)
            count++;
        break;
    }
    if(count==0)
        printf("%d number is prime",num);
    else
        printf("%d number is not prime",num);
}
